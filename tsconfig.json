{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "es6",
        "jsx": "react",
        "allowJs": false,
        "strict": true,
        "sourceMap": true,
        "types": ["node", "jest", "react", "react-dom", "gatsby"],
        "lib": ["es2015", "dom"],
        "skipLibCheck": true,
        "noUnusedLocals": true
    },
    "exclude": ["node_modules"]
}
